{
  "name": "F1FL",
  "version": "1.0.0",
  "description": "Formula 1 Fastest Lap Visualization Tool",
  "main": "app.js",
  "scripts": {
    "build": "babel server -s -D -d dist",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "node ./run-client.js",
    "server": "npm run -s build && node dist/index.js",
    "devstart": "nodemon -w server --exec \"babel-node server\"",
    "lint": "node_modules/eslint/bin/eslint.js .",
    "test": "mocha --require babel-polyfill --compilers js:babel-register --timeout 10000",
    "sqlze": "node_modules/sequelize-cli/lib/sequelize",
    "sqlze-gen": "node_modules/sequelize-auto/bin/sequelize-auto -o './models'"
  },
  "author": "mkora",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "npm": "^6.1.0",
    "sequelize": "^4.37.10",
    "winston": "^3.0.0-rc3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.2",
    "sequelize-auto": "^0.4.29",
    "sequelize-cli": "^4.0.0"
  }
}
